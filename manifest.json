{
  "name": "Fetch Review",
  "manifest_version": 2,
  "version": "0.1",
  "minimum_chrome_version": "10.0",
  "devtools_page": "devtools.html",
  "description": "Log all review from Taobao and Tmall!",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://*/*"
  ]
}